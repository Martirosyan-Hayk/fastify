{
  "name": "api-gateway",
  "version": "1.0.0",
  "author": "Hayk Martirosyan",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">= 18.15.0"
  },
  "scripts": {
    "build:prod": "tsc -p tsconfig.build.json",
    "start:dev": "cross-env TZ=UTC ts-node src/main.ts",
    "start:repl": "cross-env TZ=UTC ts-node src/repl.ts",
    "start:prod": "TZ=UTC node dist/main.js",
    "new": "hygen new",
    "watch:dev": "nest start -w",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./e2e/jest-e2e.json",
    "prepare": "node ./prepare.js"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/express": "^2.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/jwt": "^7.2.0",
    "@fastify/mongodb": "^7.0.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/one-line-logger": "^1.2.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/redis": "^6.1.1",
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/type-provider-typebox": "^3.3.0",
    "@fastify/under-pressure": "^8.3.0",
    "@fastify/websocket": "^8.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "common-crm": "./common-crm",
    "compression": "^1.7.4",
    "eslint-plugin-import": "^2.28.1",
    "express": "^4.18.2",
    "fastify": "^4.21.0",
    "fastify-jwt": "^4.2.0",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nats": "^2.16.0",
    "parse-duration": "^1.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.5.3",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@swc/core": "^1.3.66",
    "@types/cache-manager": "^4.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "lint-staged": "~13.2.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
